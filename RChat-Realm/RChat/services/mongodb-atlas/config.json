{
    "name": "mongodb-atlas",
    "type": "mongodb-atlas",
    "config": {
        "clusterName": "Cluster0",
        "readPreference": "primary",
        "sync": {
            "state": "enabled",
            "partition": {
                "key": "partition",
                "type": "string",
                "required": true,
                "permissions": {
                    "read": {
                        "%%true": {
                            "%function": {
                                "arguments": [
                                    "%%partition"
                                ],
                                "name": "canReadPartition"
                            }
                        }
                    },
                    "write": {
                        "%%true": {
                            "%function": {
                                "arguments": [
                                    "%%partition"
                                ],
                                "name": "canWritePartition"
                            }
                        }
                    }
                }
            }
        },
        "wireProtocolEnabled": false
    },
    "version": 1
}
